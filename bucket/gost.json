{
    "homepage": "https://github.com/ginuerzh/gost",
    "description": "GO Simple Tunnel - a simple tunnel written in golang",
    "version": "2.9.2",
    "persist": "config.json",
    "pre_install": "New-Item -Path $dir -Name \"config.json\" -ItemType \"file\" -Value \"# 配置文件参考：https://docs.ginuerzh.xyz/gost/getting-started/\"",
    "architecture": {
        "64bit": {
            "bin": [
                [
                    "gost-windows-amd64.exe",
                    "gost",
                    "-C $persist_dir/config.json"
                ]
            ],
            "url": "https://github.com/ginuerzh/gost/releases/download/v2.9.2/gost-windows-amd64-2.9.2.zip",
            "hash": "50b068ff2a81026e53a1ab77f2742c29e93035f0aa3190e0003daf16ab6e4092"
        },
        "32bit": {
            "bin": [
                [
                    "gost-windows-386.exe",
                    "gost",
                    "-C $persist_dir/config.json"
                ]
            ],
            "url": "https://github.com/ginuerzh/gost/releases/download/v2.9.2/gost-windows-386-2.9.2.zip",
            "hash": "039f4c311312f27db598539c300c56aafeeeabc32d051802cc45d2b2b6b62f70"
        }
    },
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/ginuerzh/gost/releases/download/v$version/gost-windows-amd64-$version.zip"
            },
            "32bit": {
                "url": "https://github.com/ginuerzh/gost/releases/download/v$version/gost-windows-386-$version.zip"
            }
        }
    }
}
