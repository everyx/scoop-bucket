{
    "homepage": "https://github.com/ginuerzh/gost",
    "description": "GO Simple Tunnel - a simple tunnel written in golang",
    "version": "2.11.3",
    "persist": "config.json",
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\config.json\")) {",
        "    New-Item -Force -Path \"$dir\" -Name \"config.json\" | Out-Null",
        "}"
    ],
    "architecture": {
        "64bit": {
            "bin": [
                [
                    "gost-windows-amd64.exe",
                    "gostc",
                    "-C $dir/config.json"
                ],
                [
                    "gost-windows-amd64.exe",
                    "gost"
                ]
            ],
            "url": "https://github.com/ginuerzh/gost/releases/download/v2.11.3/gost-windows-amd64-2.11.3.zip",
            "hash": "e0265c61ef8615da5785d6e696e51339a7ef77422a1748e8232d37e31d28ab96"
        },
        "32bit": {
            "bin": [
                [
                    "gost-windows-386.exe",
                    "gostc",
                    "-C $dir/config.json"
                ],
                [
                    "gost-windows-386.exe",
                    "gost"
                ]
            ],
            "url": "https://github.com/ginuerzh/gost/releases/download/v2.11.3/gost-windows-386-2.11.3.zip",
            "hash": "7b3d725288956317e620d23181caba3133f267c0f553365a666bfbb7b41444ed"
        }
    },
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/ginuerzh/gost/releases/download/v$version/gost-windows-amd64-$version.zip"
            },
            "32bit": {
                "url": "https://github.com/ginuerzh/gost/releases/download/v$version/gost-windows-386-$version.zip"
            }
        }
    }
}
