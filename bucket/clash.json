{
    "homepage": "https://github.com/Dreamacro/clash",
    "description": "A rule-based tunnel in Go",
    "version": "1.0.0",
    "persist": "config.yaml",
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\config.yaml\")) {",
        "    New-Item -Force -Path \"$dir\" -Name \"config.yaml\" | Out-Null",
        "}"
    ],
    "architecture": {
        "64bit": {
            "url": "https://github.com/Dreamacro/clash/releases/download/v1.0.0/clash-windows-amd64-v1.0.0.zip",
            "hash": "cdfbeebab18bdb8d9c0dd1d0b4d9fbbd904147bda83bd01fc869f9df247791f5",
            "bin": [
                [
                    "clash-windows-amd64.exe",
                    "clashd",
                    "-d $dir"
                ],
                [
                    "clash-windows-amd64.exe",
                    "clash"
                ]
            ]
        },
        "32bit": {
            "url": "https://github.com/Dreamacro/clash/releases/download/v1.0.0/clash-windows-386-v1.0.0.zip",
            "hash": "60c256b6956a395cad551335f4625715561350d3af82335748641016895dd0c9",
            "bin": [
                [
                    "clash-windows-386.exe",
                    "clashd",
                    "-d $dir"
                ],
                [
                    "clash-windows-386.exe",
                    "clash"
                ]
            ]
        }
    },
    "suggest": {
        "Dashboard": [
            "clash-dashboard",
            "yacd"
        ],
        "Daemon": "nssm"
    },
    "checkver": {
        "github": "https://github.com/Dreamacro/clash"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Dreamacro/clash/releases/download/v$version/clash-windows-amd64-v$version.zip"
            },
            "32bit": {
                "url": "https://github.com/Dreamacro/clash/releases/download/v$version/clash-windows-386-v$version.zip"
            }
        }
    }
}
