            "hash": "1bfb44794ad5fe051936609ed72aa2491047548b3247fd832eab017c33687203",
            "bin": [
                [
                    "clash-windows-amd64.exe",
                    "clashd",
                    "-d $dir"
                ],
                [
                    "clash-windows-amd64.exe",
                    "clash"
                ]
            ]
        },
        "32bit": {
            "url": "https://github.com/Dreamacro/clash/releases/download/v0.17.1/clash-windows-386-v0.17.1.zip",
