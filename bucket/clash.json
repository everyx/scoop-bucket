    "version": "0.17.1",
    "persist": "config.yaml",
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\config.yaml\")) {",
        "    New-Item -Force -Path \"$dir\" -Name \"config.yaml\" | Out-Null",
        "}"
    ],
    "architecture": {
        "64bit": {
